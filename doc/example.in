&setup
 seed = 1
/

&training
 num_epochs = 20,
 batch_size = 32,
 loss_threshold = 1.E-1,
 learning_rate = 0.01,
 momentum = 0.8,
 l1_lambda = 0.6,
 l2_lambda = 0.0
/

&convolution
 cv_num_filters = 32,
 kernel_size = 3,
 stride = 1,
 clip_norm = 2.E1,
 padding_type = "same",
 convolution_type = "standard"
/

&pool
 kernel_size = 2,
 stride = 2
/

&fully_connected
 hidden_layers = 100,
 clip_norm = 2.E1
/
